{
  "name": "st-web",
  "version": "1.0.0",
  "description": "smart-web",
  "scripts": {
    "mock": "cross-env NODE_ENV=development NODE_MOCK=mock env_config=local node build/dev-server.js",
    "dev": "cross-env NODE_ENV=development env_config=dev node build/dev-server.js",
    "prd": "cross-env NODE_ENV=development env_config=prd node build/dev-server.js",
    "build": "rimraf dist/prd/* && cross-env NODE_ENV=production env_config=prd node build/build.js",
    "build:dev": "rimraf dist/dev/* && cross-env NODE_ENV=production env_config=dev node build/build.js",
    "build:prd": "rimraf dist/prd/* && cross-env NODE_ENV=production env_config=prd node build/build.js",
    "lint": "eslint --ext .js,.vue src",
    "lint:fix": "eslint --fix --ext .js,.vue src",
    "prepublishOnly": "conventional-changelog -p angular -r 2 -i CHANGELOG.md -s",
    "release": "bash scripts/release.sh",
    "release:note": "node scripts/gen-release-note.js",
    "commit": "git-cz"
  },
  "lint-staged": {
    "linters": {
      "*.{js,vue}": [
        "eslint --fix",
        "git add"
      ]
    }
  },
  "keywords": [
    "smart",
    "store"
  ],
  "author": "yiamin",
  "private": true,
